<li>
<ul>
	{foreach $tasks task}
		<li>
			<span class="tasknumber">{$task.id_project}.{$task.tasknumber}</span>
			<span class="title">{htmlencode $task.title}</span>
			{if $task.isTrackable}
				<span class="start">{Button  onclick="Todoyu.Ext.timetracking.Headlet.Timetracking.startTask($task.id)" class="startTracking"}</span>
			{/if}
		</li>
	{/foreach}
</ul>
</li>